    var previousCounter = $('.notification-counter').text(); //8
    var notificationsCount = parseInt(previousCounter);
    
    //_________________socket.on_______________
    socket.on("notify2", (order) => {
        console.log(order); 
        notificationsCount += 1;

        Notification.requestPermission(function() {
 if (Notification.permission === 'granted') {
 // user approved.
 let notification = new Notification('مرحبا', { body: 'طلب جديد'});
 setTimeout(() => {
 notification.close()
}, 5000);
